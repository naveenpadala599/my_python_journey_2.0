def file_cursor_demo(file_name):
    with open(file_name,"rb") as f:
        print("intitial cursor position:",f.tell())
        f.seek(5)
        print("after seek(5) cursor position",f.tell())
        print("read bytes",f.read(3))
        print("cursor position after read(3)",f.tell())
file_cursor_demo("body.txt")        
