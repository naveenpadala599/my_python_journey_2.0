x1=0
x2=0
x3=0
x4=0
x5=0
x6=0
x=int(input("Enter the sample size: "))
for i in range(0,x):
    y = random.choice([1,2,3,4,5,6])
    if y == 1:
       x1+=1
    elif y == 2:
        x2+=1
    elif y == 3:
        x3+=1
    elif y == 4:
        x4+=1
    elif y == 5:
        x5+=1
    elif y == 6:
        x6+=1 
print(f"x1={x1}\nx2={x2}\nx3={x3}\nx4={x4}\nx5={x5}\nx6={x6}\n"
) 
