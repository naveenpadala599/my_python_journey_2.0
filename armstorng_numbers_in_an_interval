def armstrong(x, y):
    result=[]
    for n in range(x,y+1):
        c=0
        t=n
        while t!=0:
            c+=1
            t=t//10
        t=n    
        s=0
        for i in range(0,c):
            d=t%10
            t=t//10
            s=s+(d**c)
        if s==n:
            result.append(n)
    return result    
print(armstrong(1,1000)) 
