n1=float(input("Enter the first number: "))
n2=float(input("Enter the second number: "))
print("The sum or two number {} and {} is {}.".format(n1, n2, n1+n2))
print("The difference of the two numbers %f and %f is %f."%(n1, n2, n1-n2))
print(f"The multiplication of the two numbers {n1} and {n2} is {n1*n2}")
