import cmath
a = float(input("Enter the number a in the quadratic equation: "))
b = float(input("Enter the number b in the quadratic equation: "))
c = float(input("Enter the number c in the quadratic equation: "))
sol1=(-b+cmath.sqrt(b*b-4*a*c))/(2*a)
sol2=(-b-cmath.sqrt(b*b-4*a*c))/(2*a)
print(f"The solutions of the equation are {sol1} and  {sol2}")
