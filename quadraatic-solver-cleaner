import cmath
a = float(input("Enter the number 'a': "))
b = float(input("Enter the number 'b': "))
c = float(input("Enter the number 'c': "))
sol1 = (-b+cmath.sqrt(b*b-4*a*c))/(2*a)
sol2 = (-b-cmath.sqrt(b*b-4*a*c))/(2*a)
print(f"The solution of the given qudratic equation is {sol1.real:.2f}+{sol1.imag:.2f}j and {sol2.real:.2f}+{sol2.imag:.2f}j")

