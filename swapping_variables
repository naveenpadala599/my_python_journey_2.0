x = int(input("Enter the value of x: "))
y = int(input("Enter the value of y: "))
print("The following are the values before swaping.")
print(f"x = {x}")
print(f"y = {y}")
x, y = y, x
print("The following are the values after swaping.")
print(f"x = {x}")
print(f"y = {y}")




x = int(input("Enter the value of x: "))
y = int(input("Enter the value of y: "))
print("The following are the values before swaping.")
print(f"x = {x}")
print(f"y = {y}")
temp=x
x=y
y=temp
print("The following are the values after swaping.")
print(f"x = {x}")
print(f"y = {y}")
